{
    "manifest_version": 2,
    "name": "Anti-Bold",
    "description": "真•煞氣 🅐 割草機•卍",
    "version": "0.3.2",
    "version_name": "alpha_tri",
    "background": {
        "scripts": [
            "bg.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "jquery-3.4.1.min.js",
                "face-api.js",
                "anti-bold.js"
            ]
        }
    ],
    "commands": {
        "hide": {
            "suggested_key": "Ctrl+Shift+H",
            "description": "暫時隱藏所有 canvas"
        }
    },
    "browser_action": {
        "default_title": "Anti-Bold\n真•煞氣 🅐 割草機•卍",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ],
    "web_accessible_resources": [
        "*"
    ]
}